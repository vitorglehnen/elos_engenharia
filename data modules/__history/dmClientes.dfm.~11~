  object qryProjetos: TFDQuery
    MasterSource = dsClientes
    MasterFields = 'CL_CODIGO'
    Connection = dm.connection
    SQL.Strings = (
      'select *'
      'from projetos'
      'where pj_cliente = :cl_cliente')
    Left = 352
    Top = 8
    ParamData = <
      item
        Name = 'CL_CLIENTE'
        DataType = ftInteger
        ParamType = ptInput
        Value = Null
      end>
